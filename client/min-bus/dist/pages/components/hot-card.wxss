.card {
  margin-bottom: 25rpx;
  display: flex;
  justify-content: space-between;
  height: 100%;
  background-color: #fff;
  margin: 0 20rpx;
  margin-top: 20rpx;
  padding: 10rpx 20rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 10rpx;
  box-shadow: 2rpx 5rpx 2rpx #e5e5e5;
}
.box {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  margin-left: 20rpx;
}
.label {
  font-size: 20rpx;
}
.data-label {
  font-size: 40rpx;
}
.blue-text {
  color: #00abee;
}
